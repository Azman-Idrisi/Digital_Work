{
  "version": 2,
  "builds": [
    { "src": "Backend/api/**/*.js", "use": "@vercel/node" },
    { "src": "*.html", "use": "@vercel/static" },
    { "src": "*.css", "use": "@vercel/static" },
    { "src": "*.js", "use": "@vercel/static" }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/Backend/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
} 